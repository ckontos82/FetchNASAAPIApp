@model IEnumerable<Picture>

<table class="table table-striped rounded-3 shadow-lg">
	<thead class="thead-dark">
		<tr>
			<th>ID</th>
			<th>Title</th>
			<th>Date</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var picture in Model)
		{
			<tr>
				<td>@picture.Id</td>
				<td>@picture.Title</td>
				<td>@picture.Date</td>
			</tr>
		}
	</tbody>
</table>